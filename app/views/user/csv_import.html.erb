<div style="float:right">
	<%= link_to "My account", :controller => 'user', :action => 'home' %>
	<%= link_to "My presentations", :controller => 'index', :action => 'get' %>
	<%= link_to "Logout", :controller => 'user', :action => 'logout' %>
</div>
<% form_for :dump, :url=>{:controller=>"user", :action=>"csv_import"}, :html => { :multipart => true } do |f| -%>
	<% if (@nb_add != nil) %>
	<b><%= @nb_add.to_s + " new user(s) in the database" %></b>
	<% end %>
	<table>
		<tr>
			<td>
				<label for="dump_file">
				Select a CSV File :
				</label>
			</td>
			<td >
				<%= f.file_field :file -%>
			</td>
		</tr>
		<tr>
			<td colspan="2">
				<%= submit_tag 'Submit' -%>
			</td>
		</tr>
	</table>
<% end -%>